<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical" android:layout_width="match_parent"
    android:layout_height="match_parent">

    <LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
        android:orientation="vertical"
        android:layout_width="match_parent"
        android:layout_height="fill_parent"
        android:background="#eeeeee"
        android:weightSum="1" >

        <LinearLayout
            android:orientation="horizontal"
            android:layout_width="match_parent"
            android:layout_height="wrap_content">

            <TextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="Przyjecie"
                android:id="@+id/textView20"
                android:background="#505050"
                android:textColor="#ffffff"
                android:paddingLeft="20dp" />

        </LinearLayout>

        <LinearLayout
            android:orientation="vertical"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="20dp" >

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textAppearance="?android:attr/textAppearanceLarge"
                android:text="Kod"
                android:id="@+id/textView22"
                android:textColor="#999999" />

            <EditText
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:id="@+id/editText4"
                android:focusable="true"
                android:layout_centerInParent="true"
                android:ellipsize="start"
                android:gravity="center_horizontal"
                android:hint="@string/domyslnie5"
                android:inputType="text"
                android:singleLine="true"
                android:layout_marginLeft="50dp"
                android:layout_marginRight="50dp" />
        </LinearLayout>

        <LinearLayout
            android:orientation="horizontal"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:backgroundTint="#55da79"
            android:paddingTop="10dp"
            android:paddingBottom="10dp">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textAppearance="?android:attr/textAppearanceLarge"
                android:text="Nazwa"
                android:id="@+id/textView23"
                android:textColor="#999999" />

            <EditText
                android:layout_width="100px"
                android:layout_height="wrap_content"
                android:id="@+id/editText5"
                android:layout_weight="1"
                android:backgroundTint="#939393"
                android:backgroundTintMode="add"
                android:drawableTint="#b69a9a"
                android:elegantTextHeight="false"
                android:focusable="true"
                android:textColorHint="#ababab"
                android:layout_marginRight="100dp"
                android:layout_centerInParent="true"
                android:ellipsize="start"
                android:hint="@string/domyslnie4"
                android:inputType="text"
                android:singleLine="true"
                android:layout_marginLeft="20dp"
                android:gravity="center_horizontal" />
        </LinearLayout>

        <LinearLayout
            android:orientation="horizontal"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:backgroundTint="#55da79" >

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textAppearance="?android:attr/textAppearanceLarge"
                android:text="Stan    "
                android:id="@+id/textView26"
                android:textColor="#999999" />

            <EditText
                android:layout_width="100px"
                android:layout_height="wrap_content"
                android:id="@+id/editText8"
                android:layout_weight="1"
                android:backgroundTint="#939393"
                android:backgroundTintMode="add"
                android:drawableTint="#b69a9a"
                android:elegantTextHeight="false"
                android:focusable="true"
                android:textColorHint="#ababab"
                android:layout_marginRight="100dp"
                android:layout_centerInParent="true"
                android:ellipsize="start"
                android:hint="@string/domyslnie6"
                android:inputType="text"
                android:singleLine="true"
                android:layout_marginLeft="20dp"
                android:gravity="center_horizontal" />
        </LinearLayout>

        <LinearLayout
            android:orientation="horizontal"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:backgroundTint="#55da79"
            android:paddingTop="10dp"
            android:paddingBottom="10dp">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textAppearance="?android:attr/textAppearanceLarge"
                android:text="Ilość    "
                android:id="@+id/textView27"
                android:textColor="#999999" />

            <EditText
                android:layout_width="100px"
                android:layout_height="wrap_content"
                android:id="@+id/editText9"
                android:layout_weight="1"
                android:backgroundTint="#939393"
                android:backgroundTintMode="add"
                android:drawableTint="#b69a9a"
                android:elegantTextHeight="false"
                android:focusable="true"
                android:textColorHint="#ababab"
                android:layout_marginRight="100dp"
                android:layout_centerInParent="true"
                android:ellipsize="start"
                android:hint="@string/domyslnie6"
                android:inputType="text"
                android:singleLine="true"
                android:layout_marginLeft="20dp"
                android:gravity="center_horizontal" />
        </LinearLayout>

        <LinearLayout
            android:orientation="horizontal"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:backgroundTint="#55da79"
            android:paddingTop="10dp"
            android:paddingBottom="10dp">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textAppearance="?android:attr/textAppearanceLarge"
                android:text="Ilość zarej.:"
                android:id="@+id/textView24"
                android:textColor="#999999" />

            <EditText
                android:layout_width="100px"
                android:layout_height="wrap_content"
                android:id="@+id/editText6"
                android:layout_weight="1"
                android:backgroundTint="#939393"
                android:backgroundTintMode="add"
                android:drawableTint="#b69a9a"
                android:elegantTextHeight="false"
                android:focusable="true"
                android:textColorHint="#ababab"
                android:layout_marginRight="100dp"
                android:layout_centerInParent="true"
                android:ellipsize="start"
                android:hint="@string/domyslnie6"
                android:inputType="text"
                android:singleLine="true"
                android:layout_marginLeft="20dp"
                android:gravity="center_horizontal" />
        </LinearLayout>

        <LinearLayout
            android:orientation="horizontal"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:backgroundTint="#55da79"
            android:paddingTop="10dp">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textAppearance="?android:attr/textAppearanceLarge"
                android:id="@+id/textView25"
                android:textColor="#999999"
                android:paddingBottom="10dp"
                android:paddingTop="10dp"
                android:text="Data" />

        </LinearLayout>

        <LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            android:weightSum="1">

            <FrameLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent" >

            </FrameLayout>

            <LinearLayout
                android:orientation="horizontal"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:backgroundTint="#55da79" >

            </LinearLayout>

            <LinearLayout
                android:orientation="horizontal"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:backgroundTint="#55da79" >

            </LinearLayout>

            <LinearLayout
                android:orientation="vertical"
                android:layout_width="match_parent"
                android:layout_height="3dp"
                android:background="#646464"
                android:layout_marginTop="50dp" />

            <LinearLayout
                android:orientation="vertical"
                android:layout_width="match_parent"
                android:layout_height="match_parent">

                <LinearLayout
                    android:orientation="horizontal"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:backgroundTint="#55da79" />

                <LinearLayout
                    android:orientation="horizontal"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:background="#d6d7d7"
                    android:layout_weight="2">

                    <ImageButton
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:id="@+id/imageButton15"
                        android:src="@drawable/akceptuj2"
                        android:background="#00000000"
                        android:layout_weight="1" />

                    <ImageButton
                        android:layout_width="fill_parent"
                        android:layout_height="match_parent"
                        android:id="@+id/imageButton16"
                        android:src="@drawable/zamknij"
                        android:background="#00000000"
                        android:layout_weight="1" />
                </LinearLayout>

                <LinearLayout
                    android:orientation="vertical"
                    android:layout_width="match_parent"
                    android:layout_height="3dp"
                    android:background="#646464" />
            </LinearLayout>

        </LinearLayout>
    </LinearLayout>

</LinearLayout>