<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    tools:context="com.example.mkorpal.myapplication.ekranglowny"
    android:orientation="vertical" android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:id="@+id/LayoutMain"
    >

    <LinearLayout
        android:orientation="horizontal"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="#d6d7d7"
        android:id="@+id/topSection"
        android:layout_marginTop="30dp">

        <LinearLayout
            android:orientation="horizontal"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:weightSum="1">

            <LinearLayout
                android:orientation="horizontal"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:gravity="center">

                <TextView
                    android:layout_width="fill_parent"
                    android:layout_height="40dp"
                    android:textAppearance="?android:attr/textAppearanceLarge"
                    android:text="Nazwa Firmy 0001"
                    android:layout_marginTop="45px"
                    android:layout_marginLeft="25px"
                    android:id="@+id/textView3"
                    android:textColor="#4F5254" />
            </LinearLayout>

            <LinearLayout
                android:orientation="horizontal"
                android:layout_width="match_parent"
                android:layout_height="71dp">

                <ImageButton
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:id="@+id/imageButton"
                    android:src="@drawable/warehousegreen"
                    android:layout_gravity="right" />
            </LinearLayout>

        </LinearLayout>

    </LinearLayout>

    <LinearLayout
        android:orientation="vertical"
        android:layout_width="match_parent"
        android:layout_height="3dp"

        android:background="#646464"></LinearLayout>

    <TextView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:text="Menu"
        android:textAlignment="center"
        android:textStyle="bold"
        android:id="@+id/textView4"
        android:textColor="#ffffff"
        android:layout_gravity="center_horizontal"
        android:background="#4c4c4c" />

    <LinearLayout
        android:orientation="vertical"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/middleSection"

        android:background="#56C260">
        <ScrollView
            android:layout_width="match_parent"
            android:layout_height="372dp">
        <LinearLayout
            android:orientation="vertical"
            android:layout_width="match_parent"
            android:layout_height="match_parent">

            <Button
                android:id="@+id/button2"
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:drawableLeft="@drawable/pobieranieuzytk2"
                android:text="(1) Pobieranie użytkownika"
                android:contextClickable="true"
                android:nestedScrollingEnabled="false"
                android:background="@drawable/bgprzycisk"
                android:textColor="#ffffff" />

            <Button
                android:id="@+id/button3"
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:drawableLeft="@drawable/usuwaniebaz"
                android:text="(2) Usuwanie Baz"
                android:contextClickable="true"
                android:nestedScrollingEnabled="false"
                android:background="@drawable/bgprzycisk"
                android:textColor="#ffffff" />

            <Button
                android:id="@+id/button4"
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:drawableLeft="@drawable/zmianahasla"
                android:text="(3) Administrowanie Hasłem"
                android:contextClickable="true"
                android:nestedScrollingEnabled="false"
                android:background="@drawable/bgprzycisk"
                android:textColor="#ffffff" />

            <Button
                android:id="@+id/button5"
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:drawableLeft="@drawable/ustawieniaterminala"
                android:text="(4) Ustawienie terminala"
                android:contextClickable="true"
                android:nestedScrollingEnabled="false"
                android:background="@drawable/bgprzycisk"
                android:textColor="#ffffff" />

            <Button
                android:id="@+id/button6"
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:drawableLeft="@drawable/klawiaturaekranowa"
                android:text="(5) Klawiatura ekranowa"
                android:contextClickable="true"
                android:nestedScrollingEnabled="false"
                android:background="@drawable/bgprzycisk"
                android:textColor="#ffffff" />

            <Button
                android:id="@+id/button7"
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:drawableLeft="@drawable/webtransfer2"
                android:text="(6) Webtransfer"
                android:contextClickable="true"
                android:nestedScrollingEnabled="false"
                android:background="@drawable/bgprzycisk"
                android:textColor="#ffffff" />

            <Button
                android:id="@+id/button8"
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:drawableLeft="@drawable/slowniki"
                android:text="(7) Słowniki"
                android:contextClickable="true"
                android:nestedScrollingEnabled="false"
                android:background="@drawable/bgprzycisk"
                android:textColor="#ffffff" />

            <Button
                android:id="@+id/button9"
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:drawableLeft="@drawable/inwentaryzacja2"
                android:text="(8) Inwentaryzacja"
                android:contextClickable="true"
                android:nestedScrollingEnabled="false"
                android:background="@drawable/bgprzycisk"
                android:textColor="#ffffff" />

            <Button
                android:id="@+id/button10"
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:drawableLeft="@drawable/przyjecie2"
                android:text="(9) Przyjecie"
                android:contextClickable="true"
                android:onClick="WydanieStart"
                android:nestedScrollingEnabled="false"
                android:background="@drawable/bgprzycisk"
                android:textColor="#ffffff" />

            <Button
                android:id="@+id/button11"
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:drawableLeft="@drawable/wypozyczenie"
                android:text="(10) Wypożyczenie"
                android:contextClickable="true"
                android:onClick="WydanieStart"
                android:nestedScrollingEnabled="false"
                android:background="@drawable/bgprzycisk"
                android:textColor="#ffffff" />

            <Button
                android:id="@+id/button12"
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:drawableLeft="@drawable/zwrot"
                android:text="(11) Zwrot"
                android:onClick="WydanieStart"
                android:contextClickable="true"
                android:nestedScrollingEnabled="false"
                android:background="@drawable/bgprzycisk"
                android:textColor="#ffffff" />

            <Button
                android:id="@+id/button13"
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:drawableLeft="@drawable/wydanie2"
                android:text="(12) Wydanie"
                android:contextClickable="true"

                android:nestedScrollingEnabled="false"
                android:background="@drawable/bgprzycisk"
                android:textColor="#ffffff"
                android:onClick="WydanieStart" />


        </LinearLayout>
</ScrollView>
    </LinearLayout>

    <LinearLayout
        android:orientation="vertical"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/linearLayout4"
        android:background="#56C260"
        android:layout_marginTop="0dp" >

        <LinearLayout
            android:orientation="vertical"
            android:layout_width="match_parent"
            android:layout_height="2dp"
            android:background="#000000" />

        <LinearLayout
            android:orientation="horizontal"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:background="#d6d7d7">

            <ImageButton
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:id="@+id/imageButton12"
                android:src="@drawable/akceptuj2"
                android:background="#585858"
                android:layout_weight="1" />

            <ImageButton
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:id="@+id/imageButton13"
                android:src="@drawable/zamknij"
                android:background="#585858"
                android:layout_weight="1" />
        </LinearLayout>

    </LinearLayout>

</LinearLayout>